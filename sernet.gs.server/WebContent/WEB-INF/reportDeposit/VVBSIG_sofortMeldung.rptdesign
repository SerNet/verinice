    <method name="initialize"><![CDATA[// lets check if our javascript interpreter is capable of String.trim(), and implement it if not
if(String.prototype != null){
	if(!String.prototype.trim){  
 		String.prototype.trim = function(){  
			return this.replace(/^\s+|\s+$/g,'');  
		};  
	}
}

function isIn(list, charToTest){
	list = String(list); // ensure we are working on javascript strings, not on java.lang.string, to avoid ambiguity
	chartToTest = String(charToTest);
	
	if(list != null &amp;&amp; list.indexOf(",") != -1){
		sArr = list.split(",");
		for(var i=0; i < sArr.length; i++){
			token = sArr[i];
			token = token.trim(); // could only be used because of initial additional implementation of String.trim()
			trimmedTestChar = charToTest.trim();
			if(token.equals(trimmedTestChar)){
				return true;
			}
		}
	} else if(list != null){
		list = list.trim();
		trimmedTestChar = charToTest.trim();
		if(list.equals(trimmedTestChar)){
			return true;
		} 
	}
	return false;
}

function isEqual(a, b){
	if(new String(a).trim().equals(new String(b).trim()) ){
		return true;
	}
	return false;
}

function trim(str) {
        //return str.replace("/^\s+|\s+$/g","");
        //return str.replace(String(/(?:(?:^|\n)\s+|\s+(?:$|\n))/g),String('')).replace(String(/\s+/g),String(' '));
        return str.replace(String(/^\s+|\s+$|\s+(?=\s)/g), String(""));
}

function getOptionValuesForPropertyIDs(propertyTypeIds){
	importPackage(Packages.sernet.hui.common.connect);
	importPackage(Packages.java.util);
	importPackage(Packages.java.lang);
	importPackage(Packages.sernet.verinice.model.iso27k);
	importPackage(Packages.sernet.verinice.report.service.impl);
	importPackage(Packages.sernet.hui.common.multiselectionlist);
	
	row = new ArrayList();
    for(var i = 0; i < propertyTypeIds.length; i++) { 
    	propertyType = propertyTypeIds[i];
    	type = HitroUtil.getInstance().getTypeFactory().getPropertyType(Incident.TYPE_ID, propertyType);
        for(var j = 0; j < type.getOptions().size(); j++){
        	pOption = type.getOptions().get(j);
        	textOptionValue = pOption.toString();
	        row.add(textOptionValue != null ? textOptionValue : "") 
		}
	}
    return row;
}]]></method>
