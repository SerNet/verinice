            <xml-property name="queryText"><![CDATA[import sernet.verinice.service.commands.crud.ExecuteHQLInReportCommand;
import sernet.verinice.model.iso27k.PersonIso;
import java.util.ArrayList;
import java.util.Arrays;
import java.lang.Integer;
import java.lang.StringBuilder;
import sernet.gs.service.Retriever;

elmtid = inpv("elmtID");

elmthql =  "from CnATreeElement elmt " +
			"inner join fetch elmt.entity as entity "  +
			"inner join fetch entity.typedPropertyLists as propertylist " +
			"inner join fetch propertylist.properties as props " +
		    	"where elmt.dbId in " +
			"(select link.id.dependencyId from CnALink link where link.id.typeId = ?) " +
			" and elmt.scopeId = ?";

hqlLoader = new ExecuteHQLInReportCommand(elmthql, new Object[]{"rel_org_personiso_vv_bsig", elmtid}, PersonIso.TYPE_ID);
hqlElements = helper.execute(hqlLoader).getResult();
hqlPersons = new ArrayList();

for(Object o : hqlElements){	
	if(o instanceof PersonIso){
		person = (PersonIso) o;
		if(!hqlPersons.contains(person)){
			hqlPersons.add(person);
		}
	}
}
results = new ArrayList(0);
for(PersonIso person : hqlPersons){
	result = new StringBuilder();
	result.append(person.getName());
	surname = person.getSurname();
	if(surname != null &amp;&amp; !(surname.equals("") )){
		result.append(", ");
		result.append(surname);
	}
	results.add(result.toString());		
}
return results;]]></xml-property>
