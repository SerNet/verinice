            <xml-property name="queryText"><![CDATA[import sernet.hui.common.*;
import sernet.verinice.service.commands.crud.*;
import sernet.verinice.report.service.commands.*;
import java.util.ArrayList;
import sernet.verinice.model.common.CnATreeElement;
import sernet.verinice.model.iso27k.Organization;


// set top level element for report:
root = helper.getRoot();
// try to get manual parameter if set:
myroot = _inpv.get("root_object");
if (myroot != null &amp;&amp; myroot != -1) {
	root = myroot;
}

child=null;

hql = "from CnATreeElement org " +
                	"left join fetch org.entity as entity " + 
	                "left join fetch entity.typedPropertyLists as propertyList " + 
        	        "left join fetch propertyList.properties as props " + 
			"where org.dbId IN (" +
				"select elmt.scopeId from CnATreeElement elmt " +
				"where elmt.dbId = ?)";

c3 = new ExecuteHQLInReportCommand(hql, new Object[]{root}, CnATreeElement.class);
elementList = helper.execute(c3).getResult();

elements = new ArrayList(0);
for(Object o : elementList){
	if(o instanceof Organization){
		if(!(elements.contains(o))){
			elements.add(o);
		}
	}
}
cols = helper.getAllPropertyTypes("org");
result = helper.mapfast(
	elements,
	cols
);

return result;]]></xml-property>
