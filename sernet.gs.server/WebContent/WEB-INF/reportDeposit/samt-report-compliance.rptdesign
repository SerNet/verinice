            <xml-property name="queryText"><![CDATA[import sernet.hui.common.*;
import sernet.verinice.service.commands.crud.*;
import java.lang.Integer;
import sernet.verinice.model.common.CnATreeElement;
import java.util.ArrayList;
import sernet.verinice.model.iso27k.Audit;


// set top level element for report:
root = helper.getRoot();
// try to get manual parameter if set:
myroot = _inpv.get("root_object");
if (myroot != null &amp;&amp; myroot != -1) {
	root = myroot;
}


hql = "from CnATreeElement elmt " +
                	"left join fetch elmt.entity as entity " + 
	                "left join fetch entity.typedPropertyLists as propertyList " + 
        	        "left join fetch propertyList.properties as props " + 
			"where elmt.dbId = ?";
	params = new Object[]{root};
	loader = new ExecuteHQLInReportCommand(hql, params, CnATreeElement.class);
	elementList = helper.execute(loader).getResult();


elements = new ArrayList(0);
for(Object o : elementList){
	if(o instanceof Audit){
		if(!(elements.contains(o))){
					elements.add(o);
		}
	}
}

cols = helper.getAllPropertyTypes("audit");

//c2 = new LoadReportElements("audit", root);
//elements = helper.execute(c2).getElements();
//elements = getAllElementsHQL("audit", Integer.valueOf(root));

result = helper.mapfast(
	elements,
	cols
); 


return  result;

]]></xml-property>
