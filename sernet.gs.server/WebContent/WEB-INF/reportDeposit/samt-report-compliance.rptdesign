            <xml-property name="queryText"><![CDATA[import sernet.hui.common.*;
import sernet.verinice.model.iso27k.ControlGroup;
import sernet.verinice.model.samt.SamtTopic;
import sernet.verinice.model.iso27k.Control;
import sernet.verinice.model.iso27k.IControl;
import sernet.verinice.model.common.CnATreeElement;
import sernet.gs.service.NumericStringComparator;
import sernet.verinice.service.commands.crud.ExecuteHQLInReportCommand;
import java.util.ArrayList;
import java.util.HashMap;
import java.util.Set;
import java.util.Map.Entry;
import java.util.Collections;
import java.util.Comparator;


maturity = new HashMap(0);
typedChildrenMap = new HashMap(0);
childrenMap = new HashMap(0);
elementMap = new HashMap(0);
propMap = new HashMap(0);
itemMap = new HashMap(0);
overviewMap = new HashMap(0);
summaryMap = new HashMap(0);
mbwMap = new HashMap(0);
weightMap = new HashMap(0);
matMap = new HashMap(0);
wMatMap = new HashMap(0);
wMatMap1 = new HashMap(0);
mbwMap1 = new HashMap(0);

controlCount = 0;

// gets all children of element with dbid <id> of entitytype <type>
List getChildren(int id, String type){
	hql = "from CnATreeElement elmt " +
                	"left join fetch elmt.entity as entity " + 
	                "left join fetch entity.typedPropertyLists as propertyList " + 
        	        "left join fetch propertyList.properties as props " + 
			"where elmt.parentId = ? and elmt.typeId = ?";
	params = new Object[]{id, type};
	if(!(typedChildrenMap.containsKey(params))){
		loader = new ExecuteHQLInReportCommand(hql, params, CnATreeElement.class);
		result = helper.execute(loader).getResult();	
		typedChildrenMap.put(params, result);
		return result;
	} else {
		return typedChildrenMap.get(params);
	}
}

// gets all children of element with dbid <id> of all entities
List getChildren(int id){
	if(!(childrenMap.containsKey(id))){
		hql = "from CnATreeElement elmt " +
        	        	"left join fetch elmt.entity as entity " + 
	        	        "left join fetch entity.typedPropertyLists as propertyList " + 
        	        	"left join fetch propertyList.properties as props " + 
				"where elmt.parentId = ? ";
		params = new Object[]{id};
		loader = new ExecuteHQLInReportCommand(hql, params, CnATreeElement.class);
		result = helper.execute(loader).getResult();	
		childrenMap.put(id, result);
		return result;
	} else {
		return childrenMap.get(id);
	}
}


// gets a property of an element (using hql), given by propertyType and dbid of cnatreelement
String getElmtPropByHql(int dbid, String propType){
	elementHql = "select props.propertyValue from CnATreeElement elmt " +
                	"inner join elmt.entity as entity " + 
	                "inner join entity.typedPropertyLists as propertyList " + 
        	        "inner join propertyList.properties as props " +
			"where  elmt.dbId = ? AND props.propertyType = ?";
	params = new Object[]{dbid, propType};
	if(!(propMap.containsKey(params))){
		c2 = new ExecuteHQLInReportCommand(elementHql, params, CnATreeElement.class);
		result3 = helper.execute(c2).getResult();
		if(result3 instanceof ArrayList){
			if(result3.size() > 1){
				// remove duplicates
				s = new LinkedHashSet(result);
				result3.clear();
				// remove empty entries
				s.remove("");
				s.remove(null);
				tmpSet = new LinkedHashSet();
				for(Object entry : s){
					tmpSet.add(String.valueOf(entry).trim());
				}
				result3.addAll(tmpSet);
				if(result3.size() > 1){
					propMap.put(params, "");
					return "";
				}  else if (result3.size() == 1){
					propMap.put(params, result3.get(0));
					return result3.get(0);
				} else {
					propMap.put(params, "");
					return "";
				}
			} else {
				if(result3.size() > 0 &amp;&amp; (result3.get(0) instanceof String)){
					propMap.put(params, (String)result3.get(0));
					return (String)result3.get(0);
				}
			}
			if(result3.size() == 1 &amp;&amp; result3.get(0) instanceof String){
				propMap.put(params, (String)result3.get(0));
				return (String)result3.get(0);
			}
		}
		propMap.put(params, "");
		return "";
	} else {
		return propMap.get(params);
	}
}

// computes average of the so called "target maturity" of a given (by dbid) controlgroup
HashMap getAverageTargetMaturity(Integer topGroupId){
	map = new HashMap();
	groups = getChildren(topGroupId);
	for (CnATreeElement group : groups) {
		if(!(map.containsKey(group.getDbId()))){
			if (group instanceof ControlGroup &amp;&amp; isOverviewGroup((ControlGroup)group)){
				map.put(group.getTitle(), getAverageTargetMaturity((ControlGroup)group));
			}
		}
	}

	return map;
}

// elements with maturity = NA are not considered for computation of average of target maturity 
// target maturity in isa 2.0 catalogue equals samt topic property "threshold 2"
double getAverageTargetMaturity(ControlGroup group){
	children = getChildren(group.getDbId());
	targetMatCount = 0.0;
	topicCount = 0.0;
	tmpSet = new HashSet();
	tmpSet.addAll(children);
	children.clear();
	children.addAll(tmpSet);
	for(CnATreeElement child : children){
		if(child instanceof SamtTopic &amp;&amp; (IControl.IMPLEMENTED_NA_NUMERIC != getMaturity(child))){
				try{
					targetMatCount +=  (double)Integer.valueOf(child.getThreshold2());
					topicCount += Double.valueOf(1.0);
				} catch (e){

				}
		}
	}
	if(topicCount != 0.0){
		
		try{
			atMat = (double) targetMatCount / (double) topicCount;
			if(atMat >= 0.0){
				return atMat;
			}
		} catch (e) {
			// do nothing		
		}
	}
	return Double.valueOf(0.0);
}

// returns a map with average maturity values for each controlgroup of an isa
HashMap getItems(Integer topGroupId) {
	map = new HashMap();	
	groups = getChildren(topGroupId);
	summaryMap = new HashMap(0);
	for (CnATreeElement group : groups) {
		if(!(summaryMap.containsKey(group.getDbId()))){
			if (group instanceof ControlGroup &amp;&amp; isOverviewGroup((ControlGroup)group)){
				summary = getSummary((ControlGroup)group);
				if(summary < 0.0){
					summary = Double.valueOf(0.0);
				}
				summaryMap.put(group.getDbId(), summary);
				map.put(group.getTitle(), summary);
			}
		} else {
			map.put(group.getTitle(), summaryMap.get(group.getDbId()));		
		}
	}
	return map;
}

// consider only controlgroups that does not have property "controlgroup_is_NoIso_group" set
boolean isOverviewGroup(ControlGroup g){
	if(!(overviewMap.containsKey(g.getDbId()))){
		isOverviewElementString = getElmtPropByHql(g.getDbId(), "controlgroup_is_NoIso_group");
		if(isOverviewElementString != null &amp;&amp; ("0".equals(isOverviewElementString) || "".equals(isOverviewElementString))){
			overviewMap.put(g.getDbId(), Boolean.TRUE);
			return Boolean.TRUE;
		} else {
			overviewMap.put(g.getDbId(), Boolean.FALSE);
			return Boolean.FALSE;
		}
	} else {
		return overviewMap.get(g.getDbId());
	}
}

Double getSummary(ControlGroup group) {
	if(!(summaryMap.containsKey(group.getDbId()))){
		mw = getMaturity(group);
		//map.put(group.getTitle(), mw);
		summaryMap.put(group.getDbId(), mw);
		return mw;
	} else {
		return summaryMap.get(group.getDbId());	
	}
}

// returns avg maturity of a controlgroup
Double getMaturity(ControlGroup cg) {
	if(!(mbwMap1.containsKey(cg.getDbId()))){
		result =0.0;
		result = getMaturity(cg.getDbId());
		mbwMap1.put(cg.getDbId(), result);
		return result;
	} else {
		return mbwMap1.get(cg.getDbId());
	}
}


double getMaturity(int cgId) {
	if(!(wMatMap1.containsKey(cgId))){
		maturity1 = 0;	
		topicCount1 = 0;
		for (CnATreeElement child : getChildren(cgId)) {
			if (child instanceof IControl) {
				control = (IControl) child;
				if(IControl.IMPLEMENTED_NA_NUMERIC != getMaturity(control)){
					maturity1 += getMaturity(control);
					topicCount1++;
				}
	            	}
			if (child instanceof ControlGroup) {
				controlgroup = (ControlGroup) child;
				maturity1 += getMaturity(controlgroup.getDbId());
			}
        	}
		quotient = Double.valueOf(0.0);
		if(topicCount1 > 0) {
			quotient = (double)maturity1 / (double) topicCount1;
		}
		wMatMap1.put(cgId, quotient);
        	return quotient;
	} else {
		return wMatMap1.get(cgId);
	}
}



// returns maturity for a given control or samttopic
int getMaturity(IControl contr){
	if(!(matMap.containsKey(contr.getDbId()))){
		mat = 0;
		if(contr instanceof Control){
			mat = Integer.valueOf(getElmtPropByHql(contr.getDbId(), Control.PROP_MATURITY));
		} else if(contr instanceof SamtTopic){
			mat = Integer.valueOf(getElmtPropByHql(contr.getDbId(), SamtTopic.PROP_MATURITY));
		}
		matMap.put(contr.getDbId(), mat);
		return mat;
	} else {
		return matMap.get(contr.getDbId());
	}
}

List sort(Set entrySet) {
	list = new ArrayList();
	list.addAll(entrySet);
	Collections.sort(list, new Comparator() {
		public int compare(Entry o1, Entry o2) {
			NumericStringComparator comparator = new NumericStringComparator();
			return comparator.compare(o1.getKey(), o2.getKey());
		}
	});
	return list;
}

// set top level element for report:
root = helper.getRoot();
// try to get manual parameter if set:
myroot = _inpv.get("root_object");
if (myroot != null &amp;&amp; myroot != -1) {
	root = myroot;
}

controlgroupHQL = "select elmt.dbId from CnATreeElement elmt " + 
                	"inner join elmt.entity as entity " + 
	                "inner join entity.typedPropertyLists as propertyList " + 
        	        "inner join propertyList.properties as props " + 
                	"where elmt.parentId = ? and elmt.objectType = ? ";
	params = new Object[]{root, ControlGroup.TYPE_ID};
	idLoader = new ExecuteHQLInReportCommand(controlgroupHQL, params, CnATreeElement.class);
	result2 = helper.execute(idLoader).getResult();
	
	samtGroupId = null;
	for(Object r : result2){
		if(r instanceof Integer){
			samtGroupId = r;
		}
	}
	maturity = getItems(samtGroupId);
	entrySet = maturity.entrySet();

	avgMat = getAverageTargetMaturity(samtGroupId);
	
	result = new ArrayList(0);
	for(Entry entry : sort(entrySet)){
		row = new ArrayList(0);
		row.add(entry.getKey());
		row.add(String.valueOf(entry.getValue()));
		row.add(String.valueOf(avgMat.get(entry.getKey())));
		row.trimToSize();
		result.add(row);
	}

// adding index as value to list is needed for correct placement of topics in spider chart
for(List list : result){
	list.add(String.valueOf(result.indexOf(list)));
}

return result;]]></xml-property>
