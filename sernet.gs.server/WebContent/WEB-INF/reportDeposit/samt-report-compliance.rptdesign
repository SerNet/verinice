            <xml-property name="queryText"><![CDATA[import java.util.ArrayList;
import sernet.verinice.samt.service.TotalSecurityFigureISA2Command;
import sernet.verinice.service.commands.crud.ExecuteHQLInReportCommand;
import sernet.verinice.model.iso27k.ControlGroup;
import sernet.verinice.model.common.CnATreeElement;

// set top level element for report:
root = helper.getRoot();
// try to get manual parameter if set:
myroot = _inpv.get("root_object");
if (myroot != null &amp;&amp; myroot != -1) {
	root = myroot;
}

c = helper.execute(new TotalSecurityFigureISA2Command(root));
secfig = c.getTotalSecurityFigure();
results = new ArrayList(0);
result = new ArrayList(0);
result.add(String.valueOf(secfig));
result.add(String.valueOf(c.getReducedTargetMaturity()));
result.add(String.valueOf(c.getAverageMaximumAchieveableMaturity()));
results.add(result);
return results;]]></xml-property>
