            <xml-property name="queryText"><![CDATA[import sernet.hui.common.*;
import sernet.verinice.model.common.CnATreeElement;
import sernet.verinice.model.iso27k.PersonIso;
import sernet.verinice.model.common.CnALink;
import java.util.ArrayList;
import java.lang.Integer;
import sernet.gs.ui.rcp.main.service.crudcommands.*;
import sernet.hui.common.VeriniceContext;
import java.math.BigDecimal;
import java.math.RoundingMode;

// property loading method (hql-way)
String getElmtPropByHql(int dbid, String propType){
	elementHql = "select distinct props.propertyValue from CnATreeElement elmt " +
                	"inner join elmt.entity as entity " + 
	                "inner join entity.typedPropertyLists as propertyList " + 
        	        "inner join propertyList.properties as props " +
			"where  elmt.dbId = ? AND props.propertyType = ?";
	params = new Object[]{dbid, propType};
	c2 = new ExecuteHQLInReportCommand(elementHql, params, CnATreeElement.class);
	result3 = helper.execute(c2).getResult();
	if(result3 instanceof ArrayList){
		if(result3.size() > 1){
			// remove duplicates
			s = new LinkedHashSet(result);
			result3.clear();
			// remove empty entries
			s.remove("");
			s.remove(null);
			tmpSet = new LinkedHashSet();
			for(Object entry : s){
				tmpSet.add(String.valueOf(entry).trim());
			}
			result3.addAll(tmpSet);
			if(result3.size() > 1){
				return "";
			}  else if (result3.size() == 1){
				return result3.get(0);
			} else return "";
		} else {
			if(result3.size() > 0 &amp;&amp; (result3.get(0) instanceof String)){
				return (String)result3.get(0);
			}
		}
		if(result3.size() == 1 &amp;&amp; result3.get(0) instanceof String){
			return (String)result3.get(0);
		}
	}
	return "";
}


htf = VeriniceContext.get(VeriniceContext.HUI_TYPE_FACTORY);

// set top level element for report:
root = helper.getRoot();
// try to get manual parameter if set:
myroot = _inpv.get("root_object");
orgId = _inpv.get("orgId");
if (myroot != null &amp;&amp; myroot != -1) {
	root = myroot;
}

// hql qry to load links of type from type of elmt
personNameHql = " from CnATreeElement elmt " + 
		"left join fetch elmt.entity as entity " + 
		"left join fetch entity.typedPropertyLists as propertyList " + 
        	"left join fetch propertyList.properties as props " + 
		"where elmt.dbId IN ( " +
		"select link.id.dependencyId from CnALink link where link.id.dependantId = ? " +  
		") and elmt.scopeId = ? and elmt.objectType = ?";


// load org for root
hql = "select elmt.scopeId from CnATreeElement elmt where elmt.dbId = ?";
scopeIdLoader = new ExecuteHQLInReportCommand(hql, new Object[]{root}, CnATreeElement.class);
orgId = helper.execute(scopeIdLoader).getResult().get(0);


cols = helper.getAllPropertyTypes("samt_topic");
c2 = new LoadReportElements("samt_topic", root);
elements = helper.execute(c2).getElements();
result = helper.map(
	elements,
	cols, true
);

// compute additional fields first
orgName =  getElmtPropByHql(orgId, "org_name");
auditName = getElmtPropByHql(root, "audit_name");

sfLoader = new sernet.verinice.samt.service.TotalSecurityFigureISA2Command(root);
bd = new BigDecimal(helper.execute(sfLoader).getResult());
bd= bd.setScale(2, RoundingMode.HALF_UP);
securityFigure = String.valueOf(bd.doubleValue());

minorCount = 0;
majorCount = 0;
deviationCount = 0;

countList = new ArrayList();

// compute deviation Counts (every tuple of [topic, person] should be counted, topics without linked persons count as 1
for(i = 0; i < elements.size(); i++){
		topic = elements.get(i);

		deviation = topic.getEntity().getSimpleValue("samt_topic_audit_devi");
		c2 = new ExecuteHQLInReportCommand(personNameHql, new Object[]{topic.getDbId(), orgId, PersonIso.TYPE_ID }, PersonIso.TYPE_ID);
		persons = helper.execute(c2).getResult();
		personProps = helper.getAllPropertyTypes("person-iso").length;
		if(persons != null &amp;&amp; persons.size() > 0){
			subtrahent = personProps % 10; 
			personCount = persons.size() / (personProps - subtrahent); // divide through a value which is a multiple of 10 // assumes that # of properties of persons is > 10
			if("0".equals(deviation)){
				minorCount += (personCount);
				deviationCount += (personCount);
			} else if("1".equals(deviation)){
				majorCount += (personCount);
				deviationCount += (personCount);
			}
		} else if(persons == null || persons.size() == 0){
			if("0".equals(deviation)){
				minorCount++;
				deviationCount++;
			} else if("1".equals(deviation)){
				majorCount++;
				deviationCount++;
			}
		}	
}

//initialize final resultList
mergedResults = new ArrayList(result.size());

// now iterate topics and linked persons (if existant)
for(i = 0; i < elements.size(); i++){
	topic = elements.get(i);
	topicResult = result.get(i);
	c2 = new ExecuteHQLInReportCommand(personNameHql, new Object[]{topic.getDbId(), orgId, PersonIso.TYPE_ID }, PersonIso.TYPE_ID);
	persons = helper.execute(c2).getResult();
	if(persons != null &amp;&amp; persons.size() > 0){
		relationSB = new StringBuilder();
		relationNameSB = new StringBuilder();
		personSB = new StringBuilder();
		entryList = new ArrayList();
		for(Object o : persons){
			if(o instanceof PersonIso){
					topicPersonResult = new ArrayList(0);
					topicPersonResult.addAll(topicResult);
					person = (PersonIso)o;
					// get relationID of link via hql
					hql = "select link.id.typeId from CnALink link" +
						" where link.id.dependencyId = ? " +
							" and link.id.dependantId = ?";
					params = new Object[]{person.getDbId(), topic.getDbId()};
					linkLoader = new ExecuteHQLInReportCommand(hql, params,  CnALink.TYPE_ID);
					linkResults = helper.execute(linkLoader).getResult();
					relationID = "";
					relationName = "";
					if(linkResults == null){
						linkResults = new ArrayList(0);
					}
					for(String s : linkResults){
						relationID = s;
						relation = htf.getRelation(s);
						relationName = relation.getName();
					}
					personTitle = ((PersonIso)o).getTitle();
					valueArr = relationName + personTitle;
					if(!(entryList.contains(valueArr))){
						topicPersonResult.add(personTitle);
						topicPersonResult.add(relationID);
						topicPersonResult.add(relationName);
						// add org- and auditname				
						topicPersonResult.add(orgName);
						topicPersonResult.add(auditName);
						topicPersonResult.add(securityFigure);
						topicPersonResult.add(String.valueOf(minorCount));
						topicPersonResult.add(String.valueOf(majorCount));
						topicPersonResult.add(String.valueOf(deviationCount));
						if(!(mergedResults.contains(topicPersonResult))){
							mergedResults.add(topicPersonResult);	
						}
						entryList.add(valueArr);
					}

	



			} // end instaceof person if
		} // end of person loop
	} // end of if persons != null if

	else if(persons == null || persons.size() == 0){ // if no person linked, add padding for additional fields
		for(int k = 0; k < 3; k++){ 
			topicResult.add("");
		}
		topicResult.add(orgName);
		topicResult.add(auditName);
		topicResult.add(securityFigure);
		topicResult.add(String.valueOf(minorCount));
		topicResult.add(String.valueOf(majorCount));
		topicResult.add(String.valueOf(deviationCount));
		if(!(mergedResults.contains(topicResult))){
			mergedResults.add(topicResult);
		}
	}
		

} //end of element loop
return mergedResults;]]></xml-property>
