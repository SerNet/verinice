function tryParseDate(inputDate){
	importPackage(Packages.java.lang);
	importPackage(Packages.java.util);
	importPackage(Packages.java.text);
	try{
		d = new Date(Long.parseLong(inputDate));
		return d;
	} catch (f) {
	}
	//Wed Sep 17 00:00:00 CEST 2014
	var patterns = new Array("EEE MMM dd HH:mm:ss zzzz yyyy", "dd.MM.yy", "EE, dd.MM.yyyy", "dd.MM.yyyy", "dd.MM.yyyy HH:mm", "MMM dd, yyyy hh:mm aa", "MMM dd, yyyy hh:mm aa", "yyyy-MM-dd", "EEE MMM dd HH:mm:ss z yyyy");
	var locales = new Array(Locale.GERMAN, Locale.ENGLISH);
	for(var i = 0; i < patterns.length; i++){
		for(var j = 0; j < locales.length; j++){
			try{
				formatter = new SimpleDateFormat(patterns[i], locales[j]);
				formatter.setLenient(true);
				return formatter.parse(inputDate);
			} catch (e) {
			
			}
		}
	}
	return null;
}

function getTimeDifference(date){
	importPackage(Packages.java.util);
	importPackage(Packages.java.lang);
	importPackage(Packages.java.text);
	currentDate = new Date(System.currentTimeMillis());
	currentDate.setSeconds(0);
	currentDate.setMinutes(0);
	currentDate.setHours(0);
	dayDiff = date.getTime() - currentDate.getTime();
	days = dayDiff / 1000 / 60 / 60 / 24;
	return Math.round(days);
}

function formatDate(inputDate, destinationFormat) {
	importPackage(Packages.java.text);
	importPackage(Packages.java.util);
	importPackage(Packages.java.lang);

	locale = Locale.getDefault();
    destinationFormat = new SimpleDateFormat(destinationFormat, locale);
    return destinationFormat.format(inputDate);
}


