  <session-factory>
    <property name="connection.username">{user}</property>
    <property name="connection.url">
      {url}
    </property>
    <property name="dialect">
      {dialect}
    </property>
    <property name="connection.password">{pass}</property>
    <property name="connection.driver_class">
