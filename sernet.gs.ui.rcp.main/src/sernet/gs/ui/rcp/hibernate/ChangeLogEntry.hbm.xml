<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
	Audit log entry, also used to record changes to database items and synchronize changes on clients.
	
	@author koderman@sernet.de
 -->
<hibernate-mapping package="sernet.verinice.model.common">
	<class name="ChangeLogEntry"  table="changelogentry"
		mutable="false">
		
		<id name="dbId" 
			type="java.lang.Integer" 
			column="dbId"
			unsaved-value="null">
			<generator class="native" />
		</id>
		
		<property name="elementId"/>

		<property name="elementClass"/>

		<property name="change"
			column="elementChange"
		/>

		<property name="stationId"/>
		
		<property name="username" />

		<property name="changetime" 
			type="timestamp"
		/>
		
	</class>
	
	
</hibernate-mapping>
