	root = myroot;
}

child=null;

c3 = new LoadCnAElementById("audit", root);
child = helper.execute(c3).getFound();

if (child == null) {
	c3 = new LoadCnAElementById("org", root);
	child = helper.execute(c3).getFound();
}

cols = helper.getAllPropertyTypes("org");
c2 = new LoadReportParentOrgForObject(child);
elements = helper.execute(c2).getElements();
return helper.map(
	elements,
	cols
);
]]></xml-property>
